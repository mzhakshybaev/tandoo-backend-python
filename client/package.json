  "private": true,
  "scripts": {
    "app:start": "watchman watch-del-all && (adb reverse tcp:8081 tcp:8081 || true) && react-native start --reset-cache",
    "app:test": "jest",
    "app:asset": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "app:wipe:": "watchman watch-del-all && rm package-lock.json && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/npm-* && rm -rf ios/Pods && pod cache clean --all && rm -rf ios/build",
    "ios:run": "react-native run-ios --simulator=\"iPhone X\"",
    "android:run": "react-native run-android",
    "android:clean": "./android/gradlew clean -p ./android/",
    "android:release": "cd android && ./gradlew assembleRelease",
    "xcode open": "open ios/client.xcworkspace",
    "web:start": "webpack-dev-server --host localhost --progress --color --env.dev",
    "web:build": "rimraf ./web/build && cross-env NODE_ENV=production webpack -p --env.prod",
    "web:clean": "rimraf ./web/build",
    "web:statistics": "webpack -d --progress --watch --profile --json > compilation-stats.json --env.dev",
    "lint": "eslint .",
    "postinstall": "npx jetify"
  },
